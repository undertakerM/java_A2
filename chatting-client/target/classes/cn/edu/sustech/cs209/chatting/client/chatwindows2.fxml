<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"  >
   <children>
      <Label id="chatWithTitle" fx:id="chatWithTitle" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="450.0" text="chat with" textAlignment="CENTER" />
       <SplitPane focusTraversable="true" VBox.vgrow="ALWAYS">
          <items>
              <SplitPane dividerPositions="0.85" orientation="VERTICAL" prefHeight="400.0" prefWidth="530.0">
                 <items>
                     <ListView fx:id="chatWithContent" prefHeight="200.0" prefWidth="200.0" />
                     <HBox prefHeight="100.0" prefWidth="200.0">
                        <children>
                            <TextArea fx:id="inputWithArea" prefHeight="138.0" prefWidth="380.0" HBox.hgrow="ALWAYS" />
                            <Button maxHeight="1.7976931348623157E308" mnemonicParsing="false" onAction="#doSendMessage" prefHeight="42.0" prefWidth="50.0" text="Send" HBox.hgrow="ALWAYS" />
                        </children>
                     </HBox>
                 </items>
              </SplitPane>
          </items>
       </SplitPane>
   </children>
</VBox>
